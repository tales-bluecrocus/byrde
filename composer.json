{
	"name": "byrde/byrde",
	"description": "WordPress theme Byrde",
	"type": "wordpress-theme",
	"require": {
		"php": ">=8.0"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.14",
		"php-stubs/wordpress-stubs": "^6.8"
	},
	"scripts": {
		"fix": "php-cs-fixer fix --config=.config/.php-cs-fixer.dist.php"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
